{
	"manifest_version": 2,
	"name": "Blocklist for Duck Duck Go",
	"description": "Block domains from Duck Duck Go search results",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://duckduckgo.com/*"
			],
			"js": [
				"content_script.js"
			]
		}
	],
	"options_ui": {
		"page": "options/index.html"
	},
	"permissions":[
		"storage"
	]
}